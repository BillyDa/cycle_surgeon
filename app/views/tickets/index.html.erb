
<h1>Available Tickets</h1>

<% @tickets.each do |i| %>
   <%= 'Address: ' %><%#= i.cyclist_location%><br />
   <%= 'Repair Type: '%><%= i.repair_string%><br />
   <%= 'Description: '%><%= i.description%><br />
   <%= link_to 'Examination Room', ticket_url(i.id)%><br />
   <br />
<% end %>

<%= link_to 'Return to your dashboard', user_path(current_user) %>
